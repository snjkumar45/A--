package losTeamPractice.dayA;

public class Animal {
	//instance variable
	int a=20;
	//static variable
	void m1() {
		//instance area
	}
	
	static int b=30;
	//instance or non - static area

	public static void main(String[] args) {
		Animal animal=new Animal();
		//static area 
		System.out.println(b);
		System.out.println(animal.a);//Cannot make a static reference to the non-static field a

	}

}
class A{
	//instance area
	{
		
		System.out.println(Animal.b);
	}
	
}
