package losTeamPractice.dayA;

public class Outer {
	static private int a = 30;

	 static class Inner {
		public void m1() {
			System.out.println(a);
		}
		

	}

	public static void main(String[] args) {
		Outer o1 = new Outer();
		Outer o2 = new Outer();
		System.out.println(Outer.age);
		// System.out.println(zy);
		Outer.Inner tu = Inner.m1();

	}

	public void show() {
		{
			// anonymous inner class
		}
	}

}
