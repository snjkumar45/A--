package implementsRunnable;

public class Main {
	public static void main(String[] args) {
		
	}

}

class B implements Runnable {

	@Override
	public void run() {
		try {
			System.out.println("Thread "+Thread.currentThread().getId()+" Running");

		} catch (Exception e) {
			System.out.println(e+"excpetion caught");
		}

	}

}