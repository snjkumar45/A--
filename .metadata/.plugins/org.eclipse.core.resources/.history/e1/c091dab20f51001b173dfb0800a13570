	// static variable
	void m1() {
		// instance area
	}

	static int b = 300;
	// instance or non - static area

	public static void main(String[] args) {
		 

	}

	static {

		System.out.println("Last Position ");
	}

	/*
	 * 30 30 20
	 */

}

